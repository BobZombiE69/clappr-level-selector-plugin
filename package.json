{
  "name": "level-selector",
  "description": "A simple plugin for Clappr that adds support to manually select level on multi-bitrate streams.",
  "version": "0.1.10",
  "license": "MIT",
  "repository": "clappr/clappr-level-selector-plugin",
  "main": "index.js",
  "scripts": {
    "release": "node_modules/.bin/webpack --progress -d --optimize-minimize --optimize-dedupe --output-filename level-selector.min.js",
    "build": "node_modules/.bin/webpack --progress",
    "watch": "node_modules/.bin/webpack --progress --watch",
    "test": "karma start --single-run --browsers Chrome",
    "start": "node_modules/.bin/webpack-dev-server --content-base public/ --output-public-path /latest --hot",
    "lock": "rm -rf npm-shrinkwrap.json node_modules && npm install --silent && npm shrinkwrap"
  },
  "author": {
    "name": "Lucas Mundim",
    "email": "lucas.mundim@gmail.com",
    "url": "https://github.com/lucasmundim"
  },
  "contributors": [
    {
      "name": "Fl√°vio Ribeiro",
      "email": "email@flavioribeiro.com"
    },
    {
      "name": "Bruno Torres",
      "email": "me@brunotorr.es"
    }
  ],
  "keywords": [
    "clappr",
    "plugin"
  ],
  "dependencies": {
    "clappr": "latest",
    "clappr-zepto": "latest"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "css-loader": "^0.28.1",
    "html-loader": "^0.4.5",
    "mocha": "^3.3.0",
    "mocha-loader": "^1.1.1",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.0"
  }
}
